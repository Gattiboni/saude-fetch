{
  "operator": "seguros_unimed",
  "start_url": "https://www.segurosunimed.com.br/",
  "wait_for_any": "input[type='text'], input#cpf, input[name*='cpf']",
  "block_indicators": [
    "text=/verifique se você é humano/i",
    "text=/temporariamente indisponível/i"
  ],
  "steps": [
    {
      "action": "navigate",
      "target": "https://www.segurosunimed.com.br/",
      "timeout_ms": 20000
    },
    {
      "action": "click",
      "selector": "#onetrust-accept-btn-handler, button[id*='onetrust'], button[data-testid='accept-all'], button:has-text('Aceitar'), button:has-text('Continuar')",
      "timeout_ms": 3000,
      "optional": true
    },
    {
      "action": "wait_for_timeout",
      "timeout_ms": 300
    },
    {
      "action": "fill",
      "selector": "input#cpf, input[name*='cpf'], input[type='text']",
      "timeout_ms": 20000
    },
    {
      "action": "keypress",
      "selector": "input#cpf, input[name*='cpf'], input[type='text']",
      "key": "Enter",
      "timeout_ms": 15000
    }
  ]
}