{
  "operator": "unimed",
  "start_url": "https://www.unimed.coop.br/site/guia-medico#/",
  "wait_for_any": "#client-input",
  "block_indicators": [
    "text=/erro/i",
    "text=/muitos acessos/i"
  ],
  "steps": [
    {
      "action": "navigate",
      "target": "https://www.unimed.coop.br/site/guia-medico#/",
      "timeout_ms": 20000
    },
    {
      "action": "click",
      "selector": "#onetrust-accept-btn-handler, button[id*='onetrust'], button[data-testid='accept-all'], button:has-text('Aceitar'), button:has-text('Continuar')",
      "timeout_ms": 3000,
      "optional": true
    },
    {
      "action": "wait_for_timeout",
      "timeout_ms": 300
    },
    {
      "action": "fill",
      "selector": "#client-input",
      "timeout_ms": 20000
    },
    {
      "action": "wait_for",
      "selector": [
        "text=/UNIMED/i",
        "#NameIdenticadedUser",
        ".ClientInputWithLabel--text"
      ],
      "timeout_ms": 20000
    }
  ]
}