# Backend environment example for saude-fetch
# Database
MONGO_URL=mongodb://127.0.0.1:27017
MONGO_DB_NAME=saude_fetch

# Frontend CORS
FRONTEND_ORIGIN=*

# Throttling config for drivers (seconds)
FETCH_MIN_DELAY=0.6
FETCH_MAX_DELAY=1.2

# Simple App Credentials (login)
APP_USER=admin
APP_PASS=admin
# Secret for JWT signing (change for production)
APP_SECRET=change-me-please
