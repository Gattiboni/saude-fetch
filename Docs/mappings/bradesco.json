	<script>
$(document).ready(function () {
    $('.autocomplete-inputSearch').on('click', function () {
        if ($('#autocomplete-ul-menu').children('li').length) {
            $('.autocomplete-dropdown__search').on('click', function () {
                $($('.js-option-trigger')[1]).removeClass('disabled')
                $($('.js-option-trigger')[2]).addClass('disabled')
            })
        }
    })

    $($('.js-option-trigger')[0]).on('click', function () {
        $('.bs-alert-message-busca-refer__box').hide()
        $('.bs-plano-detalhe-nome').text('codigo - nome')
        $('.bs-plano-detalhe-segmentacao').text(' Segmentação assistencial: segmentacao ')
        $('.bs-plano-detalhe-abrangencia').text(' Abrangencia geográfica: abrangencia ')
        $('.bs-plano-detalhe-classificacao').text(' Classificação: classificacao ')
        $('.bs-plano-detalhe-status').text(' status ')
    })
});

function getClienteSePreenchido() {
    let cliente = condicaoLabelCliente();
    return cliente !== "" ? (cliente + ":") : "";
}
 
function condicaoLabelCliente() {
    let cpfOrCard = $('#searchCpfOrCard').val();
    return cpfOrCard.trim().length > 0 ? "cliente" : "";
}
 
function getCustomInteractionComponente(componente){
 
    let chave = "bs-auto-datalayer-";
    let atributos = componente.attributes;
    let custom = {}
 
    $(atributos).each(function(index, atributo){
 
        let name = atributo.name;
        let val = atributo.value;
 
        if(name.indexOf(chave) !== -1) {
 
            let eUmaFuncao = val.indexOf("$") !== -1 || val.indexOf("(") !== -1;
 
            let values = val.split("|");
            let answer = "";
 
            LOOPING(values, function(retorno){
                answer = tryTurnInFunction(retorno, answer);
            });
 
            value = answer.substring(0, answer.length-1);
 
            custom[name.replaceAll(chave, "")] = devoRetirarDoisPontosDoInicio(value);
        }
    });
 
    function LOOPING(list, method){
        for(let i=0;i<list.length;i++) {
            let item = list[i];
            method(item);
        }
    }
 
    function devoRetirarDoisPontosDoInicio(value) {
        let doisPontos = value.substring(0, 1);
        if(value.length > 1 && doisPontos === ":") {
            value = value.substring(1, value.length);
        }
        return value;
    }
 
    function tryTurnInFunction(retorno, answer) {
        try {
            answer += runArrayIfNotText(retorno);
        } catch(e){
            answer += retorno;
        }
       return answer += ":";
    }
 
    function runArrayIfNotText(value) {
        let funcao = new Function("return " + value);
        let finalResult = funcao();
        let isObject = typeof(finalResult);

        if(isObject === "object") {
            let temp = [];
            finalResult.each(function(index, v){
                let result = $(v).text();
                let first = result !== undefined && typeof(result) === "string"
                  && result.length > 0;
                let second = v.length > 0;

                if(first){
                    temp.push(removeAccent(result));
                } else if(second) {
                    temp.push(v);
                }
            });
            finalResult = JSON.stringify(temp).replaceAll("\"", "");
        } else {
            finalResult = removeAccent(finalResult);
        }

        return finalResult;
    }
 
    function removeAccent(str) {
        if(typeof str === "string") {
            return str.normalize('NFD').replace(/[\u0300-\u036f]/g, "").replace(/[^a-zA-Zs0-9-]/g, "-").toLowerCase();
        }
        return str;
    }
 
    return custom;
}
 
function onCliqueBotaoBuscar(componente) {
 
    let custom = getCustomInteractionComponente(componente);
 
    let opcaoSelecionada = $('[name=\'rede-plano\']:checked').val();
 
    switch(opcaoSelecionada) {
        case "plano" :{
            custom["plano"] = removeAccent($("#autocomplete-inputSearch").val());
            custom["rede"] = removeAccent($("#busca-refer-planos2 option:selected").text());
            break;
        }
        default: {
            custom["plano"] = removeAccent($(".bs-busca-refer__options [data='planos'] div.subtitle").text())
            break;
        }
    }
 
    let dataLayerEnvio = {
        "event": "interaction",
        "custom": custom
    }
 
    console.log("Enviando: " + JSON.stringify(dataLayerEnvio));
    window.dataLayer.push(dataLayerEnvio);
}
</script>

        
        
    

    
        





        







    
